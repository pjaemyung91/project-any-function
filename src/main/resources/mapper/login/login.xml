<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.myproject.config.security.SecurityDao">
  <select id="findUserByEmail" parameterType="String" resultType="com.example.myproject.user.dto.UserDto">
    SELECT
        id as id,
        email as email,
        pwd as pwd,
        `role` as role
    FROM
        customer
    WHERE
        email = #{email}
  </select>
</mapper>